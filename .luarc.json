{
  "runtime.path": [
    "?.lua"
  ],
  "workspace.ignoreSubmodules": false,
  "diagnostics.globals": [
    "vim"
  ],
  "diagnostics.disable": [
    "missing-fields"
  ],
  "workspake.userThirdParty": [
    ".luals/addons"
  ],
  "workspace.library": [
    "$VIMRUNTIME",
    ".luals/addons/busted/library",
    ".luals/addons/luassert/library"
  ]
}
